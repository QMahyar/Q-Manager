{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:tray:default",
    "core:tray:allow-set-icon",
    "core:tray:allow-set-menu",
    "core:tray:allow-set-tooltip",
    "dialog:default",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write"
  ]
}
